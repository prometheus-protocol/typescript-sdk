{
  "name": "@prometheus-protocol/typescript-sdk",
  "private": false,
  "version": "1.0.1",
  "type": "module",
  "description": "SDK for interacting with the Prometheus Protocol",
  "scripts": {
    "build": "npm run build:lib && npm run build:cli",
    "build:lib": "vite --config vite.config.lib.ts build",
    "build:cli": "vite --config vite.config.cli.ts build",
    "test": "vitest"
  },
  "bin": {
    "prometheus-cli": "./dist/cli.js"
  },
  "main": "./dist/server.cjs",
  "module": "./dist/server.js",
  "types": "./dist/server.d.ts",
  "exports": {
    ".": {
      "types": "./dist/server.d.ts",
      "import": "./dist/server.js",
      "require": "./dist/server.cjs"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "keywords": [
    "icp",
    "internet-computer",
    "prometheus",
    "payments",
    "oauth2"
  ],
  "author": "alice",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.0.14",
    "@types/prompts": "^2.4.9",
    "commander": "^14.0.0",
    "dotenv": "^17.2.0",
    "jsdom": "^26.1.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.7",
    "typescript": "~5.8.3",
    "vite": "^7.0.4",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^3.2.4"
  },
  "dependencies": {
    "@dfinity/agent": "^2.4.1",
    "@dfinity/identity": "^2.4.1",
    "@dfinity/identity-secp256k1": "^2.4.1",
    "@dfinity/ledger-icrc": "^2.9.1",
    "@dfinity/principal": "^2.4.1",
    "axios": "^1.10.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.1",
    "pem-file": "^1.0.1",
    "pkce-challenge": "^5.0.0",
    "prompts": "^2.4.2",
    "zod": "^4.0.5"
  }
}
